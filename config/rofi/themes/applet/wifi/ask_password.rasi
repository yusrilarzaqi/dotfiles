
@import "../colors.rasi"

* {
  background-color:       @al;
  text-color:             @fg;
	font:				 	          "Iosevka Nerd Font 10";
}

window {
  background-color:       @bg;
  width:      			      270px;
  padding:    			      25px;
  border:					        0px 0px 5px 0px;
  border-radius:          0px;
  border-color:           @ac;
  location:               center;
  x-offset:               0px;
  y-offset:               -20px;
}

entry {
  expand: 				true;
  text-color:				@ac;
}
